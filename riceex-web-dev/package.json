{
  "name": "RiceExchange",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://git.testserver.kiev.ua/vl/riceexchangeplatform.git",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "sh -ac '. ./process.env; npm-run-all -p watch-css start-js'",
    "dev": ". ./export.process.env.dev; npm-run-all -p watch-css start-js",
    "build": "sh -ac '. ./process.env.${REACT_APP_ENV}; react-app-rewired --max_old_space_size=4096 build'",
    "build:qa": "REACT_APP_ENV=qa npm run build",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:stage": "REACT_APP_ENV=stage npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "build:x": "react-scripts build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "truffle": "cd truffle && truffle compile",
    "truffle:go": "cd truffle && ./generate.sh",
    "truffle:stage": "cd truffle && truffle migrate --network stage && yarn contracts:sync",
    "truffle:dev": "cd truffle && truffle migrate --network development && yarn contracts:sync",
    "contracts:sync": "rimraf src/static/contracts && cp -R truffle/build/contracts/ src/static/contracts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-regular-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "antd": "^3.21.2",
    "array-to-tree": "^2.2.1",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.0",
    "bootstrap": "^4.3.1",
    "cleave.js": "1.4.10",
    "crypto-js": "^3.1.9-1",
    "customize-cra": "^0.4.1",
    "file-saver": "^2.0.1",
    "formdata-polyfill": "^3.0.18",
    "formik": "^1.5.8",
    "history": "^4.7.2",
    "html2pdf.js": "^0.9.0",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.4.1",
    "jsencrypt": "^3.0.0-rc.1",
    "jwt-decode": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "libphonenumber-js": "^1.7.21",
